{
  "name": "labouffe-js-sdk",
  "version": "0.0.0",
  "description": "LaBouffe API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "tslint -p .",
    "test": "jest",
    "prepublish": "yarn lint && yarn jest"
  },
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "modulePathIgnorePatterns": [
      "./dist"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {},
  "peerDependencies": {
    "rxjs": ">=6.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "jest": "^23.6.0",
    "rxjs": "^6.3.3",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^3.1.2"
  }
}
